-- Get the Mobile Infantry definition for a plain tile.
-- long_name: name of the type, e.g. 'Heavy Foot'
-- general: boolean, Need to get a type for a general.
-- return: name of the variable
function get_mobile_infanty_plain_tile(long_name, general)
  tile_name = 'tile_plain_' .. long_name:gsub(' ', '_')
  if general then
    tile_name = tile_name .. '_Gen'
  end
  mi_tile_name = tile_name .. '_MI'
  if _G[mi_tile_name] then
    return mi_tile_name
  end
  mi_tile = shallow_copy(_G[tile_name])
  mi_tile.depth = 40
  mi_tile.description = long_name .. ' Mobile Infantry'
  if mi_tile.author then
    mi_tile.author = 'Modified My Marc Pawlowsky based on ' .. mi_tile.author
  else
    mi_tile.author = ""
  end
  mi_tile.mesh = {  g_assets['dir'] .. 'troops/tile_40_40.obj'}
  file_name = string.lower(long_name):gsub(' ', '') .. '_mi'
  if general then
    file_name = file_name .. '_general'
  end
  mi_tile.player_red_tex = g_assets['dir'] .. 'troops/icons/' .. file_name .. '_red.png'
  mi_tile.player_blue_tex = g_assets['dir'] .. 'troops/icons/' .. file_name .. '_blue.png'
  _G[mi_tile_name] = mi_tile
  return mi_tile_name
end


-- Get the Light Camelry definition for a plain tile.
-- general: boolean, Need to get a type for a general.
-- return: name of the variable
function get_light_camelry_plain_tile(general)
    if general then
        return "tile_plain_Light_Camels_Bad_Horse_Gen"
    else
        return "tile_plain_Light_Camels_Bad_Horse"
    end
end
